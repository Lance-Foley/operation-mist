<div class="container mt-3">


  <table class="table table-striped table-hover table-bordered">
    <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Utilization</th>
      <th scope="col">Hours Per Week</th>
      <th scope="col">3 Month Hours</th>
      <th scope="col">6 Month Hours</th>
      <th scope="col">9 Month Hours</th>
      <th scope="col">12 Month Hours</th>
      <th scope="col">E_Rating</th>
      <th scope="col">Workload</th>


    </tr>
    </thead>
    <tbody>
    <%= render @division %>
    </tbody>
  </table>

  <div>
    <%= link_to "Edit", edit_division_path(@division), class: "btn btn-warning" %> |
    <%= link_to "Back", divisions_path, class: "btn btn-secondary" %>

  </div>
</div>
