<div id="<%= dom_id division %>">
  <tr>
    <td><%= link_to division.name, division %></td>
    <td><%= division.utilization %></td>
    <% begin %>
      <td><%= Job.where(division_name: division.name).sum(:hours_per_week) %></td>
      <td><%= Job.where(division_name: division.name).sum(:three_month_hours) %></td>
      <td><%= Job.where(division_name: division.name).sum(:six_month_hours) %></td>
      <td><%= Job.where(division_name: division.name).sum(:nine_month_hours) %></td>
      <td><%= Job.where(division_name: division.name).sum(:twelve_month_hours) %></td>
      <td><%= number_to_percentage(Job.where(division_name: division.name, completed: true).average(:e_rating), precision: 2) %></td>
      <td><%= number_to_percentage(Employee.where(division: division.name).sum(:man_hours_three_months) / Job.where(division_name: division.name).sum(:three_month_hours) * 100, precision: 2) %></td>
    <% rescue %>
      0
    <% end %>


  </tr>


</div>
