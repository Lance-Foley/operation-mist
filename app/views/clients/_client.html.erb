<div id="<%= dom_id client %>">
  <tr>
    <td ><%= link_to client.name, client, class:"text-info" %></td>
    <td><%= Project.where(client_id: client.id).count %></td>
    <td><%= client.address %></td>
    <td><%= client.future_address %></td>
    <td><%= client.phone_number %></td>
    <td><%= client.cell_phone %></td>
    <td><%= client.email %></td>
  </tr>

</div>
